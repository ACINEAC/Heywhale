#练习
import random
import string
import pandas as pd
import numpy as np

str_std=string.ascii_letters+string.digits+string.punctuation
c=[str_std[random.randint(0,len(str_std)-1)] for i in range(100)]
print('生成的字符串为:',''.join(c))
c.sort()
sta={}
for x in c:
    if x in sta.keys():
        sta[x]+=1
    else:
        sta.update({x:1})
ans=pd.DataFrame.from_dict(sta,orient='index',columns=['出现次数'])
ans=ans.reset_index().rename(columns={'index':'字符'})
pd.set_option('display.max_rows',None)
print(ans)
