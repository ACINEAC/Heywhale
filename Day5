#练习1
ans=[1,1]
for i in range(18):
    ans.append(ans[i]+ans[i+1])
print(ans)
#练习2
print('完美数如下:')
for i in range(2,10001):
    x=i
    for j in range(1,i):
        if i%j==0:
            x-=j
        if(x<0):
            break
    if(x==0):
        print(i,end=' ')
print()
#练习3
flag=True
print('100以内的素数如下:')
for i in range(2,101):
    for j in range(2,i):
        if i%j==0:
            flag=False
            break
    if flag:
        print(i,end=' ')
    flag=True
